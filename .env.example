# HomeSide Login Credentials
# The application will automatically log in and retrieve the session token
HOMESIDE_USERNAME=your_username_here
HOMESIDE_PASSWORD=your_password_here

# Client ID - AUTO-DISCOVERED! (optional, leave blank for auto-discovery)
# Will be automatically discovered after login via /api/v2/housefidlist
# Only set manually if you have multiple houses and want a specific one
# Format: 38/Account/HEM_FJV_149/HEM_FJV_Villa_149
HOMESIDE_CLIENTID=

# Polling Interval (minutes)
POLL_INTERVAL_MINUTES=15

# Seq Logging (leave blank to disable)
SEQ_URL=
SEQ_API_KEY=

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Heat Curve Controller
# When false (default): Simulation mode - logs what it would do without applying changes
# When true: Active mode - actually adjusts heat curve values
HEAT_CURVE_ENABLED=false

# MQTT Settings (leave blank to disable)
MQTT_BROKER=
MQTT_PORT=1883
MQTT_TOPIC_PREFIX=homeside
