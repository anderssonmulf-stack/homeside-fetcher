    {
      "id": 7,
      "type": "timeseries",
      "title": "Temperature History & Forecast",
      "description": "Historical temperatures (solid lines) and forecasts (dashed lines)",
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 12},
      "datasource": {"type": "influxdb", "uid": "dfb5s9mkiv5kwf"},
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "room_temperature"},
            "properties": [
              {"id": "displayName", "value": "Room"},
              {"id": "color", "value": {"fixedColor": "orange", "mode": "fixed"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "outdoor_temperature"},
            "properties": [
              {"id": "displayName", "value": "Outdoor"},
              {"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "indoor_temp"},
            "properties": [
              {"id": "displayName", "value": "Room (Forecast)"},
              {"id": "color", "value": {"fixedColor": "orange", "mode": "fixed"}},
              {"id": "custom.lineStyle", "value": {"fill": "dash", "dash": [10, 10]}},
              {"id": "custom.fillOpacity", "value": 0}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "outdoor_temp"},
            "properties": [
              {"id": "displayName", "value": "Outdoor (Forecast)"},
              {"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}},
              {"id": "custom.lineStyle", "value": {"fill": "dash", "dash": [10, 10]}},
              {"id": "custom.fillOpacity", "value": 0}
            ]
          }
        ]
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom", "showLegend": true},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"heating\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"heating_system\")\n  |> filter(fn: (r) => r._field == \"room_temperature\" or r._field == \"outdoor_temperature\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)"
        },
        {
          "refId": "B",
          "query": "from(bucket: \"heating\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"temperature_forecast\")\n  |> filter(fn: (r) => r.forecast_type == \"indoor_temp\" or r.forecast_type == \"outdoor_temp\")\n  |> pivot(rowKey: [\"_time\"], columnKey: [\"forecast_type\"], valueColumn: \"_value\")"
        }
      ]
    },
