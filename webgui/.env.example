# Svenskeb Settings GUI - Environment Configuration
# Copy this file to .env and fill in the values

# Flask
FLASK_SECRET_KEY=your-secret-key-here

# SMTP Email (one.com)
SMTP_SERVER=send.one.com
SMTP_PORT=587
SMTP_USER=noreply@svenskeb.se
SMTP_PASSWORD=your-email-password
FROM_EMAIL=noreply@svenskeb.se
FROM_NAME=Svenskeb Heating System

# Base URL for email links
BASE_URL=https://svenskeb.se

# Admin notifications (comma-separated)
ADMIN_EMAILS=your-admin-email@example.com
